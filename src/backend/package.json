{
  "name": "linkedin-profiles-gallery-backend",
  "version": "1.0.0",
  "description": "Backend service for LinkedIn Profiles Gallery application",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "seed": "ts-node prisma/seed.ts",
    "docker:build": "docker build -t linkedin-profiles-backend -f docker/Dockerfile .",
    "docker:run": "docker-compose -f docker/docker-compose.yml up"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.10.0",
    "@prisma/client": "^4.0.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-correlation-id": "^2.0.1",
    "express-prometheus-middleware": "^1.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "winston": "^3.8.0",
    "zod": "^3.21.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.0",
    "@jest/globals": "^29.5.0",
    "@jest/types": "^29.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "class-validator": "^0.14.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ioredis-mock": "^8.0.0",
    "jest": "^29.0.0",
    "openapi-types": "^12.1.0",
    "prettier": "^2.8.0",
    "prisma": "^4.0.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.0"
  }
}